{
  "name": "auth0-react-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-dotenv &&  npm-run-all --parallel spa api-server",
    "build": "react-dotenv &&  react-scripts build",
    "test": "react-dotenv &&  react-scripts test",
    "eject": "react-dotenv && react-scripts eject",
    "spa": "react-dotenv && react-scripts start",
    "server": "react-dotenv && node server.js",
    "server:dev": "react-dotenv && nodemon server.js",
    "api-server": "react-dotenv && node api-server.js",
    "api-server:dev": "react-dotenv && nodemon api-server.js",
    "dev": "react-dotenv && npm-run-all --parallel spa api-server:dev",
    "prod": "react-dotenv && npm-run-all --parallel server api-server"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@elastic/react-search-ui": "1.10.1",
    "@elastic/react-search-ui-views": "1.10.1",
    "@elastic/search-ui-app-search-connector": "1.10.1",
    "@babel/core": "^7.17.8",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "autoprefixer": "^10.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "helmet": "^4.4.1",
    "highlight.js": "^10.0.3",
    "jwks-rsa": "^1.12.2",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-router-dom": "^5.0",
    "react-scripts": "^5.0",
    "reactstrap": "^8.0",
    "react-dotenv": "^0.1.3",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0",
    "@testing-library/react": "^12.0",
    "jest-environment-jsdom": "^27.0",
    "jest-fetch-mock": "^3.0",
    "nodemon": "^2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
